#include <task2.h>

void main_task2(TASK2* task, INTERSTATE_VARS* interstate_vars)
{
	task->interstate_vars = interstate_vars;
	if(task->state == 1, interstate_vars){
		state1_task1(task);
	}
	else if(task->state == 2, interstate_vars){
		state2_task1(task);
	}
	else if(task->state == 3, interstate_vars){
		state2_task1(task);
	}
}

